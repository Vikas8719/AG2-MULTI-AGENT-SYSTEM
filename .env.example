# =============================================================================
# AG2 MULTI-AGENT SYSTEM CONFIGURATION
# =============================================================================

# -----------------------------------------------------------------------------
# AG2 & AI Configuration
# -----------------------------------------------------------------------------
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_ORG_ID=org-your-org-id
AG2_MODEL=gpt-4-turbo-preview
AG2_TEMPERATURE=0.7
AG2_MAX_TOKENS=4000

# Alternative AI Providers
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key
ANTHROPIC_MODEL=claude-3-opus-20240229

# -----------------------------------------------------------------------------
# GitHub Configuration
# -----------------------------------------------------------------------------
GITHUB_TOKEN=ghp_your_github_personal_access_token
GITHUB_USERNAME=your-github-username
GITHUB_REPO_PREFIX=ag2-generated
GITHUB_DEFAULT_BRANCH=main
GITHUB_ORG=your-github-org  # Optional

# -----------------------------------------------------------------------------
# Cloud Provider Configuration (Choose one: aws, gcp, azure)
# -----------------------------------------------------------------------------
CLOUD_PROVIDER=aws

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012
AWS_ECR_REGISTRY=123456789012.dkr.ecr.us-east-1.amazonaws.com

# GCP Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1
GCP_ZONE=us-central1-a
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json
GCP_CONTAINER_REGISTRY=gcr.io/your-project-id

# Azure Configuration
AZURE_SUBSCRIPTION_ID=your-azure-subscription-id
AZURE_RESOURCE_GROUP=your-resource-group
AZURE_LOCATION=eastus
AZURE_TENANT_ID=your-tenant-id
AZURE_CLIENT_ID=your-client-id
AZURE_CLIENT_SECRET=your-client-secret
AZURE_CONTAINER_REGISTRY=yourregistry.azurecr.io

# -----------------------------------------------------------------------------
# Kubernetes Configuration
# -----------------------------------------------------------------------------
KUBECONFIG_PATH=/home/user/.kube/config
K8S_NAMESPACE=ag2-system
K8S_CLUSTER_NAME=production-cluster
K8S_CONTEXT=production-context

# -----------------------------------------------------------------------------
# Docker Configuration
# -----------------------------------------------------------------------------
DOCKER_REGISTRY=docker.io
DOCKER_USERNAME=your-docker-username
DOCKER_PASSWORD=your-docker-password
DOCKER_EMAIL=your-email@example.com

# -----------------------------------------------------------------------------
# ArgoCD Configuration
# -----------------------------------------------------------------------------
ARGOCD_SERVER=argocd.example.com
ARGOCD_TOKEN=your-argocd-token
ARGOCD_NAMESPACE=argocd
ARGOCD_PROJECT=default

# -----------------------------------------------------------------------------
# Deployment Configuration
# -----------------------------------------------------------------------------
DEPLOYMENT_STRATEGY=rolling  # rolling, blue-green, canary
DEPLOYMENT_REPLICAS=3
DEPLOYMENT_AUTO_SCALE=true
DEPLOYMENT_MIN_REPLICAS=2
DEPLOYMENT_MAX_REPLICAS=10
DEPLOYMENT_CPU_THRESHOLD=70

# -----------------------------------------------------------------------------
# Logging Configuration
# -----------------------------------------------------------------------------
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_FILE=logs/app.log
LOG_FORMAT=json  # json, text
LOG_MAX_SIZE=100  # MB
LOG_BACKUP_COUNT=10
AGENT_LOG_DIR=logs/agents

# -----------------------------------------------------------------------------
# Database Configuration
# -----------------------------------------------------------------------------
DATABASE_TYPE=sqlite  # sqlite, postgresql, mysql, mongodb
DATABASE_URL=sqlite:///./ag2_system.db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=ag2_user
POSTGRES_PASSWORD=your-postgres-password
POSTGRES_DB=ag2_db

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password
REDIS_DB=0

# -----------------------------------------------------------------------------
# Security Configuration
# -----------------------------------------------------------------------------
SECRET_KEY=your-secret-key-min-32-characters-long
ENCRYPTION_KEY=your-encryption-key-32-bytes
JWT_SECRET=your-jwt-secret-key
JWT_ALGORITHM=HS256
JWT_EXPIRATION=3600

# -----------------------------------------------------------------------------
# Application Configuration
# -----------------------------------------------------------------------------
APP_NAME=AG2 Multi-Agent System
APP_VERSION=1.0.0
APP_ENV=development  # development, staging, production
DEBUG=true
API_HOST=0.0.0.0
API_PORT=8000
STREAMLIT_PORT=8501

# -----------------------------------------------------------------------------
# Feature Flags
# -----------------------------------------------------------------------------
ENABLE_HUMAN_APPROVAL=true
ENABLE_AUTO_DEPLOYMENT=false
ENABLE_SLACK_NOTIFICATIONS=false
ENABLE_EMAIL_NOTIFICATIONS=false
ENABLE_METRICS=true
ENABLE_TRACING=true

# -----------------------------------------------------------------------------
# Monitoring & Observability
# -----------------------------------------------------------------------------
PROMETHEUS_PORT=9090
GRAFANA_URL=http://localhost:3000
JAEGER_ENDPOINT=http://localhost:14268/api/traces
SENTRY_DSN=your-sentry-dsn

# -----------------------------------------------------------------------------
# Notification Configuration
# -----------------------------------------------------------------------------
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
SLACK_CHANNEL=#deployments
EMAIL_SMTP_HOST=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_FROM=noreply@example.com
EMAIL_PASSWORD=your-email-password
EMAIL_TO=admin@example.com

# -----------------------------------------------------------------------------
# MLflow Configuration (for ML projects)
# -----------------------------------------------------------------------------
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_EXPERIMENT_NAME=ag2-experiments
MLFLOW_ARTIFACT_LOCATION=s3://your-bucket/mlflow

# -----------------------------------------------------------------------------
# Terraform Configuration
# -----------------------------------------------------------------------------
TERRAFORM_BACKEND=s3  # s3, gcs, azurerm, local
TERRAFORM_STATE_BUCKET=your-terraform-state-bucket
TERRAFORM_STATE_KEY=ag2/terraform.tfstate
TERRAFORM_LOCK_TABLE=terraform-locks

# -----------------------------------------------------------------------------
# CI/CD Configuration
# -----------------------------------------------------------------------------
CI_PLATFORM=github-actions  # github-actions, gitlab-ci, jenkins
CI_TRIGGER_ON_PUSH=true
CI_RUN_TESTS=true
CI_RUN_SECURITY_SCAN=true
CI_DEPLOY_TO_STAGING=true
CI_DEPLOY_TO_PROD=false

# -----------------------------------------------------------------------------
# Project Generation Defaults
# -----------------------------------------------------------------------------
DEFAULT_LANGUAGE=python  # python, javascript, go, java
DEFAULT_FRAMEWORK=fastapi  # fastapi, flask, express, spring
DEFAULT_DATABASE=postgresql
DEFAULT_CACHE=redis
DEFAULT_MESSAGE_QUEUE=rabbitmq

# -----------------------------------------------------------------------------
# Rate Limiting
# -----------------------------------------------------------------------------
RATE_LIMIT_ENABLED=true
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000

# -----------------------------------------------------------------------------
# Timeouts (in seconds)
# -----------------------------------------------------------------------------
AGENT_TIMEOUT=300
HTTP_TIMEOUT=30
DEPLOYMENT_TIMEOUT=600

# -----------------------------------------------------------------------------
# Resource Limits
# -----------------------------------------------------------------------------
MAX_FILE_SIZE=100  # MB
MAX_PROJECT_SIZE=1000  # MB
MAX_CONCURRENT_AGENTS=5
MAX_CONCURRENT_PROJECTS=10

# -----------------------------------------------------------------------------
# Development & Testing
# -----------------------------------------------------------------------------
TESTING_MODE=false
MOCK_DEPLOYMENTS=false
DRY_RUN=false
VERBOSE=true
